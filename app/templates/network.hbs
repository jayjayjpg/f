<div class="container">
  <h2>Network</h2>
  <div class="row">
    <div class="col s4">
      {{#collapse-list}}
        <li class="collapsible-header grey lighten-2 white-text">Graph located SNPs:</li>
        {{#each uniqueInteractions as |snp|}}
          <li class="blue lighten-2 white-text">
            <div class="collapsible-header blue lighten-2 white-text"><i class="material-icons">filter_drama</i>{{snp.targetSnp}}</div>
            <div class="collapsible-body"><p>related to:  <strong>{{snp.target}} region</strong></p></div>
          </li>
        {{/each}}
        <li class="collapsible-header grey lighten-2 white-text">SNPS not found in the interactions matrix:</li>
        {{#each model.snps as |snp|}}
          <li>
            <div class="collapsible-header"><i class="material-icons">filter_drama</i>{{snp.rsId}}</div>
            <div class="collapsible-body"><p>lies in:  <strong>{{snp.genomicRegion}} region</strong></p></div>
          </li>
        {{/each}}
      {{/collapse-list}}
    </div>
    <div class="col s8">
 {{#if nodesAvailable}}
  {{cytoscape-graph elementData=cytoInteractions}}

  {{else}}
    No interactions found for these SNPs.
    
   {{/if}}
    </div>
  </div>
{{outlet}}
</div>
